<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DiscussionAvatarView">
        <!--头像的半径,dp为单位-->
        <attr name="radius" format="reference|integer" />
        <!--头像间的距离，dp为单位-->
        <attr name="space" format="reference|integer" />
        <!--是否是循环添加-->
        <attr name="isCycle" format="reference|boolean" />
    </declare-styleable>

</resources>